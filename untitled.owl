        <ont:Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Trikolon</ont:Name>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aus drei Kola [Satzgliedern] zusammengesetztes Satzgefuege (Duden)</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;Veni, vidi, vici&quot;</rdfs:isDefinedBy>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;Verliebt, verlobt, verheiratet&quot;</rdfs:isDefinedBy>
