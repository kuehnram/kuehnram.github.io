            </owl:Restriction>
        </rdf:type>
        <ont:einWeiteresElementDerEntgegengesetztenBedeutungWirdHinzugefuegt rdf:resource="http://webprotege.stanford.edu/ont.owl#Ganze-Nachfolgend"/>
        <ont:einWeiteresElementDerEntgegengesetztenBedeutungWirdHinzugefuegt rdf:resource="http://webprotege.stanford.edu/ont.owl#Phrasenobjekt"/>
        <ont:istLinguistischeGruppe rdf:resource="http://webprotege.stanford.edu/ont.owl#Semantisch"/>
